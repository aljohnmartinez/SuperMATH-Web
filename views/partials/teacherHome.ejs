<div class="row">
	<h5 class="center-align">
	<% if (subpage) { %>
		<div class="row">
				<div class="col s6">
					<% if (subpage == 'manageLessons') { %>
						Manage Lessons
					<% } else { %>
						<a href="manageLessons"> Manage Lessons </a>
					<% } %>
				</div>

				<div class="col s6">
					<% if (subpage == 'manageStudents') { %>
						Manage Students
					<% } else { %>
						<a href="manageStudents"> Manage Students </a>
					<% } %>
				</div>
		</div>
	<% } else { %>
		<div class="col s6">
			<a href="manageLessons"><div class="card hoverable">
				<div class="card-content">
					<span class="card-title"> Manage Lessons </span>
				</div>
			</div> </a>
		</div>
		<div class="col s6">
			<a href="manageStudents"><div class="card hoverable">
				<div class="card-content">
					<span class="card-title"> Manage Students </span>
				</div>
			</div> </a>
		</div>
	<% } %>
	</h5>
</div>